{
  "recurseDepth": 10,
  "source": {
    "include": ["./src", "./index.js"],
    "exclude": ["./lib", "./styles", "./jsdoc", "./imgs", "./assets"]
  },
  "sourceType": "module",
  "opts": {
    "recurse": true,
    "destination": "./jsdoc",
    "encoding": "utf8",
    "private": true,
    "ignoreCompilerErrors": false,
    "verbose": true
  }
}
